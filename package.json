{
  "name": "ratchagarn-omise.js",
  "version": "2.5.1",
  "description": "New version of Omise.js",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack-dev-server --hot --colors --progress --config webpack.dev.js",
    "dev:production": "webpack-dev-server --hot --colors --progress --config=./webpack.prod.js",
    "build:dev": "webpack -p --config=./webpack.dev.js",
    "build:staging": "webpack -p --config=./webpack.staging.js",
    "build:production": "webpack -p --config=./webpack.prod.js",
    "test": "NODE_ENV=test NODE_PATH=./:./src/ jest --no-cache --setupFiles dotenv/config",
    "test:watch": "NODE_ENV=test npm test -- --watch",
    "cypress": "cypress",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "precommit": "lint-staged && npm test",
    "format": "prettier-eslint --write \"src/**/*.js\""
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run format",
      "git add"
    ]
  },
  "jest": {
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git@git.omise.co:ratchagarn/omise.js.git"
  },
  "keywords": [
    "Omise.js"
  ],
  "author": "Ratchagarn Naewbuntad",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.0.1",
    "clean-webpack-plugin": "^0.1.9",
    "cypress": "^3.8.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^3.10.2",
    "file-loader": "^0.8.5",
    "husky": "^0.14.3",
    "jest": "24.7.1",
    "lint-staged": "^7.2.0",
    "moment": "^2.18.1",
    "prettier-eslint-cli": "^5.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^2.2.0",
    "uuid": "^3.0.1",
    "valid-url": "^1.0.9",
    "webpack": "^2.3.3",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.3",
    "whatwg-fetch": "^2.0.3"
  }
}
